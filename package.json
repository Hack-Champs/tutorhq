{
  "name": "tutorhq",
  "version": "1.0.0",
  "description": "Everything a tutor needs to run a successful remote tutoring business.",
  "main": "server/index.js",
  "scripts": {
    "test-client": "jest",
    "test-server": "mocha spec/server/serverSpec",
    "test": "npm run test-server && npm run test-client",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "react": "webpack -d --watch",
    "server": "nodemon server/index.js",
    "start": "node server/index.js",
    "build": "webpack --config webpack.config.js --progress --colors",
    "postinstall": "npm run build",
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hack-Champs/tutorhq.git"
  },
  "keywords": [
    "tutor"
  ],
  "author": "Hack-Champs",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^22.1.0",
    "babel-loader": "^6.3.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "jest": "^22.1.4",
    "mocha": "^5.0.0",
    "react-test-renderer": "^16.2.0",
    "regenerator-runtime": "^0.11.1",
    "supertest": "^3.0.0",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "aos": "^2.2.0",
    "axios": "^0.17.1",
    "body-parser": "^1.17.2",
    "chai": "^4.1.2",
    "connect-mongo": "^2.0.1",
    "dotenv": "^4.0.0",
    "express": "^4.15.0",
    "express-session": "^1.15.6",
    "jquery": "^3.1.1",
    "lodash": "^4.17.5",
    "moment": "^2.20.1",
    "mongoose": "^4.13.10",
    "nodemailer": "^4.4.2",
    "passport": "^0.4.0",
    "passport-google-oauth": "^1.0.0",
    "passport-google-oauth20": "^1.0.0",
    "rc-time-picker": "^3.2.1",
    "react": "^16.2.0",
    "react-day-picker": "^7.0.7",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "react-scrollable-anchor": "^0.5.0",
    "request": "^2.83.0",
    "semantic-ui-react": "^0.77.2"
  },
  "bugs": {
    "url": "https://github.com/Hack-Champs/tutorhq/issues"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx$": "babel-jest",
      "^.+\\.js$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "setupFiles": [
      "./spec/jestsetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "homepage": "https://github.com/Hack-Champs/tutorhq#readme"
}
